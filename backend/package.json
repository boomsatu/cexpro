{
  "name": "cex-backend",
  "version": "1.0.0",
  "description": "CEX Cryptocurrency Exchange Backend API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "build": "babel src -d dist",
    "docker:build": "docker build -t cex-backend .",
    "docker:run": "docker run -p 3001:3001 cex-backend",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:force": "node scripts/setup-database.js --force",
    "db:setup:alter": "node scripts/setup-database.js --alter",
    "db:verify": "node scripts/verify-database-schema.js",
    "db:reset": "node scripts/setup-database.js --force --alter"
  },
  "keywords": [
    "cryptocurrency",
    "exchange",
    "trading",
    "blockchain",
    "api",
    "nodejs",
    "express"
  ],
  "author": "CEX Development Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "big.js": "^6.2.1",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "ccxt": "^4.1.64",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "geoip-lite": "^1.4.10",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "ip-range-check": "^0.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^13.2.0",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^7.2.0",
    "redis": "^4.6.10",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "sharp": "^0.33.1",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "thirty-two": "^1.0.2",
    "tiny-secp256k1": "^2.2.4",
    "twilio": "^4.19.0",
    "ua-parser-js": "^2.0.4",
    "umzug": "^3.6.1",
    "useragent": "^2.3.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "web3": "^4.2.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "no-unused-vars": "error"
    },
    "env": {
      "node": true,
      "jest": true
    }
  }
}
